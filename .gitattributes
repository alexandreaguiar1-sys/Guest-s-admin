function nocollision(Parented,Part)
    for _,v in pairs(Parented:GetChildren()) do
        if v:IsA("BasePart") then
            local nocol = Instance.new("NoCollisionConstraint")
            nocol.Part0 = Part
            nocol.Part1 = v
            nocol.Parent = Part
        end
    end
end

